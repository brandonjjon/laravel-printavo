{
    "name": "brandonjjon/laravel-printavo",
    "description": "Laravel package providing an Eloquent-like interface to the Printavo GraphQL API",
    "keywords": ["laravel", "printavo", "graphql", "api", "eloquent"],
    "homepage": "https://github.com/brandonjjon/laravel-printavo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Jones"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Brandonjjon\\Printavo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brandonjjon\\Printavo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage",
        "format": "pint",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brandonjjon\\Printavo\\PrintavoServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
